{
    "name": "behzadbabaei/translation",
    "description": "A Translation package for Laravel 12 with database and cache support",
    "keywords": [
        "behzadbabaei",
        "laravel-translator",
        "laravel",
        "translator",
        "translation",
        "localization"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Behzad Babaei",
            "email": "behzadbabaei69@gmail.com",
            "homepage": "http://www.behzadbabaei.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.0",
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Behzadbabaei\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Behzadbabaei\\Translation\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
